<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<BorderPane prefWidth="400" prefHeight="150" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.jaspercloud.sdwan.platform.ui2.UpdateVersionController">
    <top>
        <Label fx:id="label" text="更新中" BorderPane.alignment="CENTER" prefWidth="400">
            <BorderPane.margin>
                <Insets bottom="10.0"/>
            </BorderPane.margin>
        </Label>
    </top>
    <center>
        <VBox>
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
            <ProgressBar fx:id="progress" prefWidth="400" prefHeight="30" progress="0" focusTraversable="false"/>
        </VBox>
    </center>
    <bottom>
        <VBox>
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
            <Button fx:id="btn" prefWidth="400" prefHeight="30" focusTraversable="false"/>
        </VBox>
    </bottom>
    <padding>
        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
    </padding>
</BorderPane>
