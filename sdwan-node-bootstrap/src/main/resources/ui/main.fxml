<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.jaspercloud.sdwan.platform.ui.MainWindowController">
    <children>
        <Label fx:id="statusLab" text="status: not install"/>
        <Button fx:id="startBtn" prefHeight="30" prefWidth="120" text="start">
            <FlowPane.margin>
                <Insets top="20.0"/>
            </FlowPane.margin>
        </Button>
        <Button fx:id="stopBtn" prefHeight="30" prefWidth="120" text="stop">
            <FlowPane.margin>
                <Insets top="20.0"/>
            </FlowPane.margin>
        </Button>
        <Button fx:id="uninstallBtn" prefHeight="30" prefWidth="120" text="uninstall" disable="true">
            <FlowPane.margin>
                <Insets top="20.0"/>
            </FlowPane.margin>
        </Button>
    </children>
</FlowPane>
